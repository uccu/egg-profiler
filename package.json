{
  "name": "egg-profiler",
  "version": "1.0.0",
  "description": "provides egg bindings for the v8 profiler.",
  "eggPlugin": {
    "name": "eggProfiler"
  },
  "keywords": [
    "egg",
    "profiler",
    "egg-profiler",
    "egg-plugin"
  ],
  "dependencies": {
    "@types/mocha": "^10.0.1",
    "v8-profiler-next": "^1.9.0"
