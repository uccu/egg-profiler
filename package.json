  },
  "devDependencies": {
    "egg": "^3",
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12",
    "mocha": "^10.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "config",
    "app.js",
    "index.d.ts"
  ],
  "ci": {
    "version": "14, 16, 18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uccu/egg-profiler.git"
  },
  "bugs": {
    "url": "https://github.com/uccu/egg-profiler/issues"
